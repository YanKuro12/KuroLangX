{
  "name": "KuroLangX",
  "scopeName": "source.kuro",
  "fileTypes": ["kuro", "kuroc"],
  "patterns": [
    { "include": "#keywords" },
    { "include": "#strings" },
    { "include": "#comments" }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.kuro",
          "match": "\\b(FuncGroup|var|import|return|if|else)\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.kuro",
          "begin": "\"",
          "end": "\""
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.kuro",
          "match": "#.*$"
        }
      ]
    }
  }
}
    "comments": {
      "patterns": [
        {
          "name": "comment.line.kuro",
          "match": "#.*$"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.kuro",
          "match": "\\b[A-Za-z_][A-Za-z0-9_]*(?=\\()"
        }
      ]
    }
}
